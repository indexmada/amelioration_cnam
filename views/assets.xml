<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend"
              name="Amelioration_cnam backend assets"
              inherit_id="website.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript"
                    src="/amelioration_cnam/static/src/js/unit_enseigne.js"/>
        </xpath>
    </template>

    <template id="assets_login" inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/amelioration_cnam/static/src/css/style.css"/>
        </xpath>
    </template>

</odoo>